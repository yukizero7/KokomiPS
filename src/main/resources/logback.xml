<Configuration>
  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>[%d{HH:mm:ss}] [%highlight(%level)] %msg%n</pattern>
    </encoder>
  </appender>
  <logger name="org.reflections" level="OFF"/>
  <root level="INFO">
    <appender-ref ref="STDOUT" />
  </root>
</Configuration>